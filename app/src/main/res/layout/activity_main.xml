<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".mvp.views.activities.MainActivity" android:background="#B3C4FF">


    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recycler_dog_breed_list"
            android:layout_width="384dp"
            android:layout_height="637dp"
            android:scrollbars="vertical"
            android:fillViewport="true"
            android:clipToPadding="false"
            android:scrollbarStyle="outsideOverlay"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>